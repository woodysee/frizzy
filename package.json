{
  "name": "frizzy",
  "version": "0.6.0",
  "description": "Reusable UI component library for Airfrov web",
  "private": true,
  "dependencies": {
    "csso": "^3.5.1",
    "lodash": "^4.17.11",
    "node-sass-chokidar": "^1.3.3"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "style-loader": "^0.21.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^2.1.5"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "build": "npm run build-css && sh compile-styles.sh && webpack",
    "start": "npm run build-css && webpack --config webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/Airfrov/frizzy.git"
  },
